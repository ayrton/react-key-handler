{
  "name": "react-key-handler",
  "version": "1.1.0",
  "description": "React component to handle keyboard events",
  "homepage": "https://github.com/ayrton/react-key-handler",
  "author": {
    "name": "Ayrton De Craene",
    "email": "im@ayrton.be",
    "url": "https://github.com/ayrton"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ayrton/react-key-handler.git"
  },
  "bugs": {
    "url": "https://github.com/ayrton/react-key-handler/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config ./webpack.build.config.js --mode production",
    "clean": "rimraf dist",
    "demo": "parcel ./demo/index.html",
    "demo:build": "parcel build demo/index.html --out-dir demo/dist --public-url=/react-key-handler",
    "demo:deploy": "npm run demo:build && gh-pages -d demo/dist",
    "flow:copy": "flow-copy-source lib dist",
    "prepublish": "npm run clean && npm run build && npm run flow:copy",
    "test": "npm run test:lint && npm run test:unit && npm run test:flow",
    "test:flow": "flow check",
    "test:lint": "eslint demo/ lib/ test/",
    "test:unit": "mocha"
  },
  "keywords": [
    "react",
    "reactjs",
    "key",
    "event",
    "handler"
  ],
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-1": "^6.13.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.77.0",
    "flow-copy-source": "^2.0.2",
    "gh-pages": "^1.2.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.4",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {
    "exenv": "^1.2.0",
    "prop-types": "^15.6.2"
  }
}
